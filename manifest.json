{
    "name": "KnowBot",
    "description": "a extension which helps to read articles/webpages efficiently",
    "version": "1.0",
    "manifest_version": 3,
  "content_scripts": [
    {
      "js": ["script.js"],
      "css": ["styles.css"],
      "matches":["*://*/*"]
    }
  ],
  "icons": {
    "16": "image_16x16.png",
    "48": "image_48x48.png",
    "128": "image_128x128.png"
  }
  }